{
    "name": "uretral-laravel-admin-ext/media-manager",
    "version": "1.0",
    "repositories": [{
        "type": "vcs",
        "url": "https://github.com/uretral/laravel-dmin-ext-media-manager.git"
    }],
    "description": "Media manager for laravel",
    "type": "library",
    "keywords": ["laravel-admin", "media", "manager"],
    "homepage": "https://github.com/laravel-admin-extensions/media-manager",
    "license": "MIT",
    "authors": [
        {
            "name": "z-song",
            "email": "zosong@126.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "encore/laravel-admin": "~1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Encore\\Admin\\Media\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Encore\\Admin\\Media\\MediaServiceProvider"
            ]
        }
    }
}
